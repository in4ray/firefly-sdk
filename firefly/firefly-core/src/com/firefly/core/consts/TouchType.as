package com.firefly.core.consts
{
	/** The TouchType describes different types of touch. This class uses in scrollers.*/	
	public class TouchType
	{
		/** The event type idetifies touch began. */		
		public static const BEGAN:String = "began";
		
		/** The event type idetifies touch continue. */
		public static const MOVED:String = "moved";
		
		/** The event type idetifies touch ended. */		
		public static const ENDED:String = "ended";
	}
}